// Auto-generated GraphQL operations for {{ schema.table }}
// Do not edit manually. Generated by generate.py

{% set exported_names = [] %}
{% for op in operations %}
  {% if op.name not in exported_names %}
    export const {{ op.name }} = /* GraphQL */ `
    {{ op.gql.strip() }}
    `;
    {% set _ = exported_names.append(op.name) %}
  {% endif %}
{% endfor %}

// For each secondary index, generate a query operation
{% for index in schema.secondary_indexes %}
  {% set export_name = schema.table + 'QueryBy' + index.partition|to_pascal_case %}
  {% if export_name not in exported_names %}
    export const {{ export_name }} = /* GraphQL */ `
    query {{ export_name }}($input: {{ export_name }}Input!) {
      {{ export_name }}(input: $input) {
        StatusCode
        Message
        Data {
{% for attr in schema.attributes %}          {{ attr.name|to_camel_case }}
{% endfor %}        }
      }
    }
    `;
    {% set _ = exported_names.append(export_name) %}
  {% endif %}
{% endfor %} 