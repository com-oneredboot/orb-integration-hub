// Generated TypeScript registry model for {{ schema.name }}
// Generated at {{ now() }}

export interface IErrorRegistry {
  code: string;
  message: string;
  description: string;
  solution: string;
  details?: any;
}

export const ERRORS: Record<string, IErrorRegistry> = {
{% for code, entry in schema.items.items() %}
  "{{ code }}": {
    code: "{{ code }}",
    message: "{{ entry.message }}",
    description: "{{ entry.description }}",
    solution: "{{ entry.solution }}"{% if entry.details is defined %},
    details: {{ entry.details | tojson }}{% endif %}
  },
{% endfor %}
};

export function getError(code: string): IErrorRegistry | undefined {
  return ERRORS[code];
} 