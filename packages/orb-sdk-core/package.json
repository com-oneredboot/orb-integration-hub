{
  "name": "@orb/sdk-core",
  "version": "1.0.0",
  "description": "Core TypeScript SDK for orb-integration-hub - Authentication, Authorization, and API client",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "orb",
    "sdk",
    "authentication",
    "authorization",
    "cognito",
    "appsync",
    "graphql"
  ],
  "author": "orb-integration-hub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/com-oneredboot/orb-integration-hub.git",
    "directory": "packages/orb-sdk-core"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.500.0",
    "graphql-request": "^6.1.0",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
